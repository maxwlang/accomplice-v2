{
  "name": "accomplice",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.11.0",
    "emojilib": "^3.0.10",
    "mariadb": "^3.1.2",
    "ramda": "^0.29.0",
    "redis": "^4.6.6",
    "sequelize": "^6.31.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "yarn clean && yarn lint && yarn build && node dist/index.js",
